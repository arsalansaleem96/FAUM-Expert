<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.faum.faum_user.Deal_Detail_one"
    tools:layout_editor_absoluteY="25dp"
    android:background="#f5d6d6"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/tvDealDetailTile"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:text="Deal Detail"
        android:background="#e18484"
        android:theme="@style/ThemeOverlay.MyTitleText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="1dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvDDOTileCategory"
        android:layout_width="0dp"
        android:layout_height="28dp"
        android:text="Deal Category"
        android:gravity="center"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/tvDDOTileDealDescton"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="159dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="159dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvDealDetailTile" />

    <TextView
        android:id="@+id/tvDDDishNamevalue"
        android:layout_width="0dp"
        android:layout_height="29dp"
        android:gravity="center"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/tvDDOTileDealDescton"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="66dp"
        app:layout_constraintTop_toBottomOf="@+id/tvDDCategoryvalue"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="67dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/tvDDOTileDealDescton"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:text="Dish Description"
        android:gravity="center"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btnDealDetailOne"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="131dp"
        app:layout_constraintTop_toBottomOf="@+id/tvDDCategoryvalue"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="130dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/tvDDDishDescvalue"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:gravity="center"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="163dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp" />

    <TextView
        android:id="@+id/tvDDNamevalue"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:gravity="center"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/tvDDOTileCategory"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="30dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvDDOTileName"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp" />

    <TextView
        android:id="@+id/tvDDOTileName"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:text="Deal Name"
        android:gravity="center"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/tvDDOTileCategory"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="78dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="79dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="1dp" />

    <TextView
        android:id="@+id/tvDDOTileDishName"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="Dish Name"
        android:gravity="center"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/tvDDOTileDealDescton"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="64dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="65dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvDDCategoryvalue" />

    <TextView
        android:id="@+id/tvDDCategoryvalue"
        android:layout_width="0dp"
        android:layout_height="28dp"
        android:gravity="center"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/tvDDDishDescvalue"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="188dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="188dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDealDetailOne"
        android:layout_width="206dp"
        android:layout_height="48dp"
        android:text="More"
        android:textColor="@drawable/buttontext"
        android:background="@drawable/hover"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="65dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/tvDDDishDescvalue"
        android:layout_marginBottom="59dp"
        android:layout_marginRight="65dp" />
</android.support.constraint.ConstraintLayout>
